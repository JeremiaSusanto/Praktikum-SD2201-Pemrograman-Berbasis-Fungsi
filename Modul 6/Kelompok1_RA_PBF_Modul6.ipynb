      "source": [
        "import math\n",
        "\n",
        "def hitung_sudut_pembiasan(sudut_potong, n1, n2):\n",
        "    theta_i = math.radians(sudut_potong)\n",
        "    theta_t = math.asin(n1 * math.sin(theta_i) / n2)\n",
        "    return math.degrees(theta_t)\n",
        "\n"
      ]
    }
  ]
